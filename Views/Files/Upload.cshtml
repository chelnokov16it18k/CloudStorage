@model IEnumerable<CloudStorage.Models.FileModel>

@{
    ViewBag.Title = "Добавление Файла";
}
<form asp-action="Upload" asp-controller="Files"  method="post" enctype="multipart/form-data">
    <p>
        <label>Файл</label>
        <input name="UploadedFile" type="file" class="form-control" />
    </p>
    <p>
        <input type="submit" value="Добавить" />
    </p>
</form>
